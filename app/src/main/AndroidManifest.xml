<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="id.tnp2k.kiatcam" >

    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature android:name="android.hardware.camera" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="id.tnp2k.kiatcam.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="id.tnp2k.kiatcam.CameraActivity"
            android:label="@string/title_activity_camera" >
        </activity>
        <activity
            android:name="id.tnp2k.kiatcam.TeacherListActivity"
            android:label="@string/title_activity_teacher_list" >
        </activity>
        <activity
            android:name="id.tnp2k.kiatcam.PreviewActivity"
            android:label="@string/title_activity_preview" >
        </activity>
        <activity
            android:name="id.tnp2k.kiatcam.FinishActivity"
            android:label="Foto selesai" >
        </activity>
        <activity
            android:name="id.tnp2k.kiatcam.TeacherListActivity2"
            android:label="@string/title_activity_teacher_list_activity2" >
        </activity>
    </application>

</manifest>
